{
  "redirects": [
    {
      "source": "/:path*/page/1",
      "destination": "/:path*",
      "permanent": true
    },
    {
      "source": "/:path*/index.html",
      "destination": "/:path*",
      "permanent": true
    }
  ],
  "functions": {
    "api/*.js": {
      "runtime": "vercel-edge"
    },
    "api/**/*.js": {
      "runtime": "vercel-edge"
    }
  }
}